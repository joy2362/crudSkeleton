{
    "name": "joy2362/crud-skeleton",
    "description": "Simple package that help complete crud faster",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4.0"
    },
    "autoload": {
        "psr-4": {
            "Joy2362\\CrudSkeleton\\": "src/"
        }
    },
    "authors": [
        {
            "name": "joy2362",
            "email": "abdullahzahidjoy@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Joy2362\\CrudSkeleton\\CrudSkeletonServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
